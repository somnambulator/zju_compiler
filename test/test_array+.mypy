def foo(a[]:int)->void:{

    i:int;

    for i in range(0,10):{
    
    
        a[i] := i+1;
        print("in foo: a[",i,"]=",a[i]);
    }
}

def main()->int:{

    a[10]:int;
    i:int;

    foo(a[]);
    for i in range(0,10):{
    
    
        a[i] := a[i]+1;
        print("in main: a[",i,"]=",a[i]);
    }
    return 0;
}