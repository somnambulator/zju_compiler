class Dog: {
    a: int, b: float;
    def __init__(self)->int:{
        self.a := 1;
        self.b := 2.0;
    }
    def getb(self)->float:{
        ret:float := self.b;
        return ret;
    }
};

def main()->int: {
    a: class Dog;
    a.__init__();
    print(a.a);
    print("\n");
    print(a.getb());
}