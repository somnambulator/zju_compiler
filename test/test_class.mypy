class Dog: {
    a: int, b: float;
    def __init__(self, a:int)->int:{
        self.a := a;
        self.b := 2.0;
    }
    def getb(self)->float:{
        ret:float := self.b;
        return ret;
    }
};

def main()->int: {
    a: class Dog;
    a.__init__(1);
    print(a.a);
    print(a.getb());
}